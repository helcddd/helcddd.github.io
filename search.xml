        self.textBrowser_6.append(url)</span><br><span class="line"></span><br><span class="line">            <span class="keyword">if</span> <span class="built_in">len</span>(names) == <span class="number">0</span> <span class="keyword">and</span> <span class="built_in">len</span>(q) != <span class="number">0</span>:</span><br><span class="line">                self.textBrowser.append(<span class="string">&quot;没有找到小说!&quot;</span>)</span><br><span class="line"></span><br><span class="line"></span><br><span class="line"></span><br><span class="line">        self.pushButton.clicked.connect(get)</span><br><span class="line"></span><br><span class="line">        self.retranslateUi(Dialog)</span><br><span class="line">        QtCore.QMetaObject.connectSlotsByName(Dialog)</span><br><span class="line"></span><br><span class="line">    <span class="keyword">def</span> <span class="title function_">retranslateUi</span>(<span class="params">self, Dialog</span>):</span><br><span class="line">        _translate = QtCore.QCoreApplication.translate</span><br><span class="line">        Dialog.setWindowTitle(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="string">&quot;Dialog&quot;</span>))</span><br><span class="line">        self.pushButton.setText(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="string">&quot;搜索&quot;</span>))</span><br><span class="line">        self.label.setText(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="string">&quot;书名&quot;</span>))</span><br><span class="line">        self.label_2.setText(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="string">&quot;作者&quot;</span>))</span><br><span class="line">        self.label_3.setText(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="string">&quot;最新章&quot;</span>))</span><br><span class="line">        self.label_4.setText(_translate(<span class="string">&quot;Dialog&quot;</span>, <span class="